    "@storybook/react": "^5.2.6",
    "autoprefixer": "^9.6.4",
    "better-docs": "^1.3.3",
    "chai-enzyme": "^1.0.0-beta.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-react-intl": "^2.0.6",
    "galenframework": "^2.4.4",
    "jsdoc": "^3.6.3",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "postcss-modules": "^1.4.1",
    "prettier": "1.19.1",
    "react-intl-cra": "^0.3.4",
    "react-intl-translations-manager": "^5.0.3",
    "sinon": "^7.5.0",
    "storybook-addon-intl": "^2.4.1",
    "tailwindcss": "^1.1.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
